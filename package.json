{
  "name": "financial-control-system",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng lint && ng build --configuration production",
    "watch": "ng lint && ng build --watch --configuration development",
    "test": "ng test",
    "tree": "tree -l 100 --ignore node_modules --ignore .angular --ignore dist -o directory-tree.txt",
    "lint": "ng lint",
    "post-release": "ts-node ./version.ts && git c \"chore(env): update enviroment versions\" && git push --tags",
    "release:ma": "standard-version --release-as major && npm run post-release",
    "release:mi": "standard-version --release-as minor && npm run post-release",
    "release:p": "standard-version --release-as patch && npm run post-release"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.12",
    "@angular/cdk": "^16.2.12",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/fire": "^16.0.0",
    "@angular/forms": "^16.2.0",
    "@angular/material": "^16.2.12",
    "@angular/material-moment-adapter": "^16.2.12",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/pwa": "^0.0.1",
    "@angular/router": "^16.2.0",
    "@angular/service-worker": "^16.2.0",
    "firebase": "^10.13.0",
    "md5-typescript": "^1.0.5",
    "ngx-colors": "^3.6.0",
    "ngx-cookie-service": "^16.1.0",
    "ngx-mask": "^16.4.1",
    "ngx-toastr": "^19.0.0",
    "rxjs": "~7.8.0",
    "standard-version": "^9.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.10",
    "@angular-eslint/builder": "^17.1.0",
    "@angular-eslint/eslint-plugin": "^17.1.0",
    "@angular-eslint/eslint-plugin-template": "^17.1.0",
    "@angular-eslint/schematics": "^16.3.1",
    "@angular-eslint/template-parser": "^17.1.0",
    "@angular/cli": "^16.2.10",
    "@angular/compiler-cli": "^16.2.0",
    "@types/jasmine": "~4.0.0",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^2.8.4",
    "typescript": "~5.1.3"
  }
}
